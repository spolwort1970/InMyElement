            <Border Background="#25BDC0">
                <Grid Width="230"
                      Height="230"
                      Margin="10">
                    <TextBlock Text="{Binding Name}"
                               HorizontalAlignment="Center"
                               VerticalAlignment="Center" />
                </Grid>
            </Border>
        </DataTemplate>
    </Page.Resources>
