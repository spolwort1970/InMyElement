<Page x:Name="pageRoot"
      x:Class="InMyElement.PuzzlePage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:local="using:InMyElement"
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
      mc:Ignorable="d"
      >
    <Page.Resources>
        <!-- TODO: Delete this line if the key AppName is declared in App.xaml -->
        <x:String x:Key="AppName">In My Element</x:String>
        <DataTemplate x:Key="ItemTemplate1">
            <Border Background="#25BDC0">
                <Grid Width="230"
                      Height="230"
                      Margin="10">
                    <TextBlock Text="{Binding Name}"
                               HorizontalAlignment="Center"
                               VerticalAlignment="Center" />
                </Grid>
            </Border>
        </DataTemplate>
    </Page.Resources>
    <Page.BottomAppBar>
        <CommandBar IsOpen="True" 
                    IsSticky="True"
                    x:Name="BottomAppBar">
            <AppBarButton x:Name="Instructions"
                          Label="Instructions"
                          Click="Instructions_Click"
                          Icon="Help" />
            <CommandBar.SecondaryCommands>
                <AppBarButton x:Name="NewGame"
                              Icon="Add"
                              Label="New Game"
                              Click="NewGame_Click" />
            </CommandBar.SecondaryCommands>
        </CommandBar>
    </Page.BottomAppBar>
    <Viewbox  Stretch="Fill" >
        <Grid x:Name="ElementsGrid"
              
              Height="768"
              Width="1366">
            
            <Grid.RowDefinitions>
                <RowDefinition Height="140" />
                <RowDefinition Height="*" />
            </Grid.RowDefinitions>
            <Grid.Background>
                <ImageBrush ImageSource="/Assets/background.jpg"
                            Stretch="UniformToFill" />
            </Grid.Background>
            <!-- Back button and page title -->
            <Grid>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="120" />
                    <ColumnDefinition Width="*" />
                </Grid.ColumnDefinitions>
                <Button x:Name="backButton"
                        Margin="39,59,39,0"
                        Command="{Binding NavigationHelper.GoBackCommand, ElementName=pageRoot}"
                        Style="{StaticResource NavigationBackButtonNormalStyle}"
                        VerticalAlignment="Top"
                        AutomationProperties.Name="Back"
                        AutomationProperties.AutomationId="BackButton"
                        AutomationProperties.ItemType="Navigation Button" />
                <TextBlock x:Name="pageTitle"
                           Text="{StaticResource AppName}"
                           Style="{StaticResource HeaderTextBlockStyle}"
                           Grid.Column="1"
                           IsHitTestVisible="false"
                           TextWrapping="NoWrap"
                           VerticalAlignment="Bottom"
                           Margin="0,0,30,40" />
            </Grid>
            <GridView x:Name="xElementGrid"
                      ItemsSource="{Binding Elements}"
                      SelectionMode="None"
                      CanDragItems="False"
                      IsItemClickEnabled="False"
                      DragItemsStarting="xElements_DragItemsStarting"
                      Drop="xElements_Drop"
                      Width="250"
                      RenderTransformOrigin="1,0.72"
                      Margin="1106,13,10,154"
                      Grid.Row="1">
                <GridView.ItemTemplate>
                    <DataTemplate>
                        <Border Background="#25BDC0">
                            <Grid Width="30"
                                  Height="30"
                                  Margin="4">
                                <TextBlock Text="{Binding Symbol}"
                                           TextAlignment="Center" />
                            </Grid>
                        </Border>
                    </DataTemplate>
                </GridView.ItemTemplate>
                <GridView.ItemsPanel>
                    <ItemsPanelTemplate>
                        <ItemsWrapGrid MaximumRowsOrColumns="18" />
                    </ItemsPanelTemplate>
                </GridView.ItemsPanel>
            </GridView>
            <Popup x:Name="InstructionsPopup"
                   Grid.Row="1"
                   IsOpen="False"
                   VerticalAlignment="Center"
                   Height="400">
                <Grid Height="400"
                      Background="Black"
                      HorizontalAlignment="center"
                      Width="1366">
                    <TextBlock Text="Instructions:"
                               VerticalAlignment="Top"
                               HorizontalAlignment="Center"
                               Margin="0,10,0,0"
                               Style="{StaticResource HeaderTextBlockStyle}"
                               TextAlignment="Left"/>
                    <StackPanel VerticalAlignment="Top"
                                HorizontalAlignment="Left"
                                Margin="10,100,10,0">
                        <TextBlock Text="Drag elements from the panel on the right to their proper place in the periodic table."
                                   Style="{StaticResource HeaderTextBlockStyle}"
                                   TextAlignment="Left" LineHeight="60"/>
                        <TextBlock Text="Click the 'Validate' button when you are done to view your score!"
                                   Style="{StaticResource HeaderTextBlockStyle}"
                                   TextAlignment="Left"
                                   LineHeight="60" />
                    </StackPanel>
                    <AppBarButton x:Name="CloseInstructions"
                                  Icon="Cancel"
                                  Click="CloseInstructions_Click"
                                  Margin="1263,10,0,0" />
                </Grid>
            </Popup>
            <Popup x:Name="GameOverPopup"
                   Grid.Row="1"
                   IsOpen="False">
                <Grid Background="Black"
                      Width="880"
                      HorizontalAlignment="Center"
                      VerticalAlignment="Center"
                      Margin="250,50,0,0"
                      Height="411">
                    <TextBlock Text="Puzzle Mode Results"
                               Style="{StaticResource HeaderTextBlockStyle}"
                               VerticalAlignment="Top"
                               HorizontalAlignment="Center"
                               Margin="0,10,0,0" />
                    <TextBlock x:Name="xUserName"
                               VerticalAlignment="Top"
                               HorizontalAlignment="Left"
                               Style="{StaticResource HeaderTextBlockStyle}"
                               Margin="10,100,0,0" />
                    <TextBlock Text="Score: "
                               VerticalAlignment="Top"
                               HorizontalAlignment="Left"
                               Style="{StaticResource HeaderTextBlockStyle}"
                               Margin="10,170,0,0" />
                    <TextBlock x:Name="CorrectAmount"
                               VerticalAlignment="Top"
                               HorizontalAlignment="Left"
                               TextAlignment="Left"
                               Style="{StaticResource HeaderTextBlockStyle}"
                               Margin="303,170,0,0" />
                    <TextBlock Text="Time: "
                               VerticalAlignment="Top"
                               HorizontalAlignment="Left"
                               Style="{StaticResource HeaderTextBlockStyle}"
                               Margin="10,240,0,0" />
                    <TextBlock x:Name="xPuzzleTime"
                               VerticalAlignment="Top"
                               HorizontalAlignment="Left"
                               TextAlignment="Left"
                               Style="{StaticResource HeaderTextBlockStyle}"
                               Margin="303,240,0,0" />
                    <AppBarButton x:Name="CloseGamePopup"
                                  Icon="Cancel"
                                  Click="CloseGamePopup_Click"
                                  Margin="776,-4,0,0" />
                </Grid>
            </Popup>
            <Popup x:Name="NewGamePopup"
                   IsOpen="True"
                   Grid.Row="1"
                   Margin="0,54,0,285">
